{
  "name": "pulls",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "generate-schema": "npx --package=json-schema-to-typescript json2ts ./config-schema.json -o ./src/main/config.d.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev-window": "electron-vite dev -m window",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@emotion/styled": "^11.11.5",
    "@minoru/react-dnd-treeview": "^3.4.4",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@reecelucas/react-use-hotkeys": "^2.0.0",
    "electron-store": "^8.0.1",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^14.1.0",
    "menubar": "^9.5.0",
    "react-dnd": "^16.0.1",
    "react-hotkeys-hook": "^4.5.0",
    "react-redux": "^9.1.2",
    "vite-plugin-commonjs-externals": "^0.1.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
