{
  "name": "pulls",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "preinstall": "export NPM_CONFIG_REGISTRY=\"\"",
    "start": "electron app.js",
    "clean": "rm -r .cache dist node_modules packaged",
    "build": "parcel settings-editor.html index.html --target electron --public-url ./",
    "package": "electron-packager . pulls --platform=darwin --out packaged --extendInfo ./buildFiles/Info.plist --overwrite",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "<rootDir>",
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "balloon-css": "^1.0.4",
    "electron-store": "^8.0.1",
    "gravitons-sass": "^1.2.0",
    "menubar": "^9.0.0",
    "mousetrap": "^1.6.5",
    "node-fetch": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sweetalert": "^2.1.2",
    "to-mils": "^1.0.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^27.3.1",
    "electron": "^15.3.1",
    "electron-packager": "^14.2.1",
    "jest": "^27.3.1",
    "jest-css-modules-transform": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  }
}
